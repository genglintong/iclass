<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.soft.domain.Subject" table="subject">
	<id name="subjectId" column="subjectId">
		<generator class="native"></generator>
	</id>
	<property name="quanzhong" column="quanzhong"/>
	<property name="subjectDesc" column="subjectDesc" length="200"/>
	<property name="createDate" column="createDate" />
	<property name="deadLine" column="deadLine"/>
	<property name="subjectTitle" column="subjectTitle"/>
	<property name="courseId" column="courseId"/>
	<!-- 配置关联关系 -->
	<!-- ss -->
	
</class>


</hibernate-mapping>
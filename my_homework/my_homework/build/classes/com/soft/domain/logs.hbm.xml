<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.soft.domain.logs" table="logs">
	<composite-id> 
            <key-property name="studentId" column="studentId"/> 
            <key-property name="courseId" column="courseId"/> 
            <key-property name="callNumber" column="callNumber" />
    </composite-id> 
	
	<property name="isComing" column="isComing" />
	<property name="creatDate" column="creatDate" />
	
	<!-- 配置关联关系 -->
</class>


</hibernate-mapping>